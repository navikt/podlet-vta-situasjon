{
  "name": "podlet-vta-situasjon",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "6.1.1",
    "@metrics/prometheus-consumer": "3.0.1",
    "@podium/podlet": "4.4.14",
    "@sentry/browser": "6.2.3",
    "classnames": "2.3.0",
    "craco-less": "1.17.1",
    "express": "4.17.1",
    "less": "3.13.1",
    "lodash.throttle": "4.1.1",
    "nav-frontend-alertstriper": "3.0.51",
    "nav-frontend-alertstriper-style": "2.0.38",
    "nav-frontend-core": "5.0.11",
    "nav-frontend-ikoner-assets": "2.0.10",
    "nav-frontend-js-utils": "1.0.17",
    "nav-frontend-knapper": "2.0.32",
    "nav-frontend-knapper-style": "1.0.17",
    "nav-frontend-lenker": "1.0.60",
    "nav-frontend-lenker-style": "0.2.42",
    "nav-frontend-paneler": "2.0.32",
    "nav-frontend-paneler-style": "0.3.34",
    "nav-frontend-typografi": "2.0.39",
    "nav-frontend-typografi-style": "1.0.35",
    "prom-client": "13.1.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "swr": "0.5.4"
  },
  "devDependencies": {
    "@sentry/cli": "1.63.1",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@testing-library/user-event": "12.8.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "msw": "0.27.1",
    "prettier": "2.2.1",
    "react-scripts": "4.0.3"
  },
  "scripts": {
    "start": "NODE_ENV=development craco start",
    "build": "REACT_APP_SENTRY_RELEASE=$(git rev-parse --short HEAD) craco build",
    "test": "npm run lint && craco test",
    "test-ci": "npm run lint && CI=true craco test",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm i",
    "podlet": "node podlet.js",
    "lint": "eslint --ext .js src",
    "sentry-release": "(export SENTRY_RELEASE=$(git rev-parse --short HEAD); node scripts/sentry-release)",
    "bump": "npx npm-check-updates --target minor -u && npm i"
  },
  "eslintConfig": {
    "plugins": [
      "jsx-a11y"
    ],
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:jsx-a11y/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "printWidth": 120
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test-ci"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,md,ts,tsx}": "prettier --write"
  }
}
